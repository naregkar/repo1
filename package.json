{
  "name": "client",
  "version": "2.29.2",
  "private": true,
  "proxy": "http://localhost:4200",
  "scripts": {
    "analyze": "react-app-rewired build && source-map-explorer 'build/static/js/*.js'",
    "start": "react-app-rewired start",
    "start:sdk": "env-cmd -f .env.sdk react-app-rewired start",
    "start-https": "HTTPS=true SSL_CRT_FILE=picsartweb.apps.pem SSL_KEY_FILE=picsartweb.apps-key.pem react-app-rewired start",
    "build": "react-app-rewired build",
    "build:staging": "env-cmd -f .env.staging react-app-rewired build",
    "build:preproduction": "env-cmd -f .env.preproduction react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "datadog:sourcemaps": "env-cmd -f .env.production datadog-ci sourcemaps upload ./build/maps --service website-editor --minified-path-prefix https://picsart.com/create/editor-static/js --release-version 2.29.0",
    "datadog:upload": "./postbuild.sh && npm run datadog:sourcemaps && rm -r ./build/maps",
    "datadog:fakeupload": "",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "precommit": "eslint --fix --max-warnings 0",
    "lint-github": "eslint 'src/**/*.{js,ts,tsx}'",
    "lint-report": "eslint --ext .js,.ts,.tsx src/ -f json -o report.json"
  },
  "dependencies": {
    "@PicsArtWeb/editor-lib": "^3.1.24",
    "@PicsArtWeb/pilibs": "12.16.48",
    "@datadog/browser-rum": "^1.18.1",
