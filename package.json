    "classnames": "^2.2.6",
    "crypto-js": "^4.0.0",
    "file-saver": "^2.0.2",
    "js-cookie": "^2.2.1",
    "jspdf": "^2.3.1",
    "jszip": "^3.4.0",
    "konva": "7.2.2",
    "lodash": "^4.17.19",
    "mime": "^2.4.5",
    "opentype.js": "^1.3.3",
    "piexifjs": "^1.0.6",
    "rc-switch": "^1.9.0",
    "react": "^17.0.1",
    "react-beautiful-dnd": "^13.0.0",
